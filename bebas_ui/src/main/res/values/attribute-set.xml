<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="text" format="string" />
    <attr name="hint" format="string" />
    <attr name="label" format="string" />

    <declare-styleable name="BebasRoundedButton">
        <attr name="text" />
        <attr name="active" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BebasOutlinedButton">
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="BebasEdittext">
        <attr name="hint" />
        <attr name="label" />
        <attr name="android:imeOptions" />
        <attr name="android:inputType" />
        <attr name="android:drawableStart" />
    </declare-styleable>

    <declare-styleable name="BebasPhoneNumberEdittext">
        <attr name="hint" />
        <attr name="label" />
        <attr name="android:imeOptions" />
        <attr name="android:inputType" />
        <attr name="android:drawableStart" />
    </declare-styleable>

    <declare-styleable name="BebasBackButton" />
</resources>